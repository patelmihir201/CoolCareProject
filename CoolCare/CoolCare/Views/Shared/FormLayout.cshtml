
@{
    ViewBag.Title = "FormLayout";
}




<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Latest compiled and minified CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>

    <!-- JQuery CDN -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
    @*<link rel="stylesheet" href="HomePage.css" />*@


    @Styles.Render("~/Content/CoolCare/aos.css")

    @Styles.Render("~/Content/CoolCare/assets/vendor/bootstrap/css/bootstrap.min.css")
    @Styles.Render("~/Content/CoolCare/assets/vendor/bootstrap-icons/bootstrap-icons.css")
    @Styles.Render("~/Content/CoolCare/assets/vendor/glightbox/css/glightbox.min.css")
    @Styles.Render("~/Content/CoolCare/assets/vendor/swiper/swiper-bundle.min.css")
    @Styles.Render("~/Content/CoolCare/css/style.css")

    <title>Technician Personal Details</title>
</head>

<body>

    <video id="bgVideo" autoplay loop muted>
        <source src="~/Content/img/BodyBackground.mp4" type="video/mp4" />
    </video>
    @RenderBody()

    <script src="~/Scripts/CoolCare/assets/js/UserLogin.js"></script>
    @Scripts.Render("~/Scripts/CoolCare/assets/vendor/purecounter/purecounter.js")
    @Scripts.Render("~/Scripts/CoolCare/aos.js")
    @Scripts.Render("~/Scripts/CoolCare/assets/vendor/bootstrap/js/bootstrap.bundle.min.js")
    @Scripts.Render("~/Scripts/CoolCare/assets/vendor/glightbox/js/glightbox.min.js")
    @Scripts.Render("~/Scripts/CoolCare/assets/vendor/isotope-layout/isotope.pkgd.min.js")
    @Scripts.Render("~/Scripts/CoolCare/assets/vendor/swiper/swiper-bundle.min.js")
    @*@Scripts.Render("~/Scripts/CoolCare/main.js")*@
    <script src="~/Scripts/CoolCare/main.js"></script>
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>

</html>